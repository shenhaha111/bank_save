/* pages/boxfee/boxfee.wxss */
.box1 {
    border-bottom: 1px solid rgb(248, 242, 242);
}

.boxshape {
    margin: 15px;
    font-weight: 600;
}

.scroll-view_H {
    white-space: nowrap;
}

.scroll-view-item_H {
    display: inline-block;
    width: 40%;
    height: 180px;
    border: 2px solid rgb(248, 242, 242);
    border-radius: 10px;
    margin: 10px;
}

.scroll-view-item_H:active {
    border-color: blue;
}

.scroll-view_H image {
    height: 30px;
    width: 30px;
    margin: 10px 40%;
    /* background-color: rgb(25, 22, 207);
    border-radius: 50%; */
}

.box2 {
    display: flex;
    flex-direction: column;
    margin-top: 15px;
}

.innerbox1 {
    display: flex;
    border-top: 1px solid rgb(235, 229, 229);
    border-bottom: 1px solid rgb(235, 229, 229);
    height: 50px;
    margin: 0px 15px;
    font-size: 14px;
    color: rgb(117, 111, 111);
}

.innerbox1 .txt {
    display: flex;
    align-items: center;
}

.innerbox1 .los {
    display: flex;
    width: 220px;
    margin-left: 20px;
    align-items: center;
    text-align: center;
}

.innerbox1 image {
    width: 16px;
    height: 16px;
    display: flex;
    align-items: center;
    margin: auto;
    float: right;
    /* margin: 0 0 0 80%; */
}

.innerbox2 {
    display: flex;
    margin-left: 15px;
    margin: 15px 15px;
    align-items: center;
}

button {
    margin-top: 30px;

}